,CAAA,GAAA,CAAA,GAAA;AACA,gBAAA,MAAA,CAAA,mBAAA,GAAA,GAAA,CAAA,IAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA,KA7CA;AA8CA,IAAA,cA9CA,4BA8CA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,qBAAA,EAAA,MAAA,CAAA,cAAA,CADA;;AAAA;AAEA,gBAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;;AACA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA,EAHA,CAIA;;;AACA,gBAAA,MAAA,CAAA,WAAA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAMA,KApDA;AAqDA,IAAA,OArDA,mBAqDA,EArDA,EAqDA;AAAA;;AACA,WAAA,QAAA,CAAA,oBAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,kBAAA,EAAA;AACA,UAAA,EAAA,EAAA;AADA,SAAA;;AAGA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,SADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;;AAIA,QAAA,MAAA,CAAA,WAAA;AACA,OAbA,EAaA,KAbA,CAaA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,MADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OAlBA;AAoBA;AA1EA;AAxCA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n     <!-- 面包屑导航 -->\r\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n      <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>仓储管理</el-breadcrumb-item>\r\n      <el-breadcrumb-item>入库管理</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n    <el-card class=\"box-card\">\r\n    <!-- 搜索与添加区域 -->\r\n      <el-row :gutter=\"10\">\r\n        <el-col :span=\"6\">\r\n          <el-input\r\n            clearable\r\n            v-model=\"queryInfo.query\"\r\n            @clear=\"getDataList\"\r\n            placeholder=\"请输入查询信息\"\r\n            class=\"input-with-select\"\r\n          >\r\n            <el-button\r\n              @click=\"getDataList\"\r\n              slot=\"append\"\r\n              icon=\"el-icon-search\"\r\n            ></el-button>\r\n          </el-input>\r\n        </el-col>\r\n        <el-col :span=\"2\">\r\n          <el-button type=\"primary\" @click=\"addDialogVisible = true\"\r\n            >新增入库信息</el-button\r\n          >\r\n        </el-col>\r\n      </el-row>\r\n      <!-- 数据列表 -->\r\n        <el-table\r\n      :data=\"tableData\"\r\n      style=\"width: 100%\">\r\n      <el-table-column\r\n        type=\"index\"\r\n        label=\"编号\"\r\n      >\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"supply_name\"\r\n        label=\"供应商\"\r\n        width=\"180\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"cpmc\"\r\n        label=\"入库货物\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"name\"\r\n        label=\"入库仓库\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"in_number\"\r\n        label=\"入库数量\">\r\n      </el-table-column>\r\n       <el-table-column\r\n        prop=\"add_date\"\r\n        label=\"入库时间\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"comment\"\r\n        label=\"备注\">\r\n      </el-table-column>\r\n        <el-table-column label=\"操作\" width=\"180px\">\r\n          <!-- 插槽自定义样式 -->\r\n          <template slot-scope=\"scope\">\r\n            <el-tooltip effect=\"dark\" content=\"编辑\" placement=\"top-start\">\r\n              <el-button\r\n                @click=\"editInfo(scope.row.id)\"\r\n                type=\"primary\"\r\n                icon=\"el-icon-edit\"\r\n              ></el-button>\r\n            </el-tooltip>\r\n            <el-tooltip effect=\"dark\" content=\"删除\" placement=\"top-end\">\r\n              <el-button\r\n                type=\"warning\"\r\n                @click=\"delInfo(scope.row.id)\"\r\n                icon=\"el-icon-delete\"\r\n              ></el-button>\r\n            </el-tooltip>\r\n          </template>\r\n        </el-table-column>\r\n    </el-table>\r\n       <!-- 分页区域 -->\r\n      <el-pagination\r\n        background\r\n        layout=\"prev, pager, next,total\"\r\n        :total=\"total\"\r\n        :page-size=\"queryInfo.pageSize\"\r\n        @current-change=\"handleCurrentChange\"\r\n      >\r\n      </el-pagination>\r\n       <!-- 新增投入品台账dialog -->\r\n      <el-dialog\r\n        title=\"新增入库信息\"\r\n        :visible.sync=\"addDialogVisible\"\r\n        width=\"40%\"\r\n      >\r\n        <!-- 表单区 -->\r\n        <el-form size=\"medium\"  ref=\"addClientInfo\" :inline=\"true\" :model=\"addClientInfo\" label-width=\"120px\">\r\n          <el-form-item label=\"供应商名称\"  prop=\"supply_name\">\r\n            <el-input v-model=\"addClientInfo.supply_name\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"入库货物\"  prop=\"cpmc\">\r\n            <el-input v-model=\"addClientInfo.cpmc\"></el-input>\r\n          </el-form-item>\r\n           <el-form-item label=\"入库仓库\"  prop=\"storage_id\">\r\n            <el-select v-model=\"addClientInfo.storage_id\" placeholder=\"请选择\">\r\n    <el-option\r\n      v-for=\"item in selectStorageOption\"\r\n      :key=\"item.id\"\r\n      :label=\"item.name\"\r\n      :value=\"item.id\">\r\n    </el-option>\r\n  </el-select>\r\n          </el-form-item>\r\n          \r\n          <el-form-item label=\"入库数量\"  prop=\"in_number\">\r\n            <el-input v-model=\"addClientInfo.in_number\"></el-input>\r\n          </el-form-item>\r\n              <el-form-item label=\"入库日期\"  prop=\"add_date\">\r\n            <el-date-picker\r\n              value-format=\"yyyy-MM-dd\"\r\n              format=\"yyyy-MM-dd\"\r\n              v-model=\"addClientInfo.add_date\"\r\n              type=\"date\"\r\n              placeholder=\"选择日期\"\r\n            >\r\n            </el-date-picker>\r\n          </el-form-item>\r\n           <el-form-item label=\"备注\"  prop=\"comment\">\r\n            <el-input v-model=\"addClientInfo.comment\"></el-input>\r\n          </el-form-item>\r\n        </el-form>\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"addDialogVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"addFormSubmit\">确 定</el-button>\r\n        </span>\r\n      </el-dialog>\r\n       <!-- 编辑投入品台账dialog -->\r\n       <el-dialog\r\n        title=\"编辑入库信息\"\r\n        :visible.sync=\"editDialogVisible\"\r\n        width=\"40%\"\r\n      >\r\n        <!-- 表单区 -->\r\n        <el-form size=\"medium\"  ref=\"editFormRef\" :inline=\"true\" :model=\"editClientInfo\" label-width=\"120px\">\r\n          <el-form-item label=\"供应商名称\"  prop=\"supply_name\">\r\n            <el-input v-model=\"editClientInfo.supply_name\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"入库货物\"  prop=\"cpmc\">\r\n            <el-input v-model=\"editClientInfo.cpmc\"></el-input>\r\n          </el-form-item>\r\n           <el-form-item label=\"入库仓库\"  prop=\"storage_id\">\r\n            <el-select v-model=\"editClientInfo.storage_id\" placeholder=\"请选择\">\r\n    <el-option\r\n      v-for=\"item in selectStorageOption\"\r\n      :key=\"item.id\"\r\n      :label=\"item.name\"\r\n      :value=\"item.id\">\r\n    </el-option>\r\n  </el-select>\r\n          </el-form-item>\r\n          \r\n          <el-form-item label=\"入库数量\"  prop=\"in_number\">\r\n            <el-input v-model=\"editClientInfo.in_number\"></el-input>\r\n          </el-form-item>\r\n              <el-form-item label=\"入库日期\"  prop=\"add_date\">\r\n            <el-date-picker\r\n              value-format=\"yyyy-MM-dd\"\r\n              format=\"yyyy-MM-dd\"\r\n              v-model=\"editClientInfo.add_date\"\r\n              type=\"date\"\r\n              placeholder=\"选择日期\"\r\n            >\r\n            </el-date-picker>\r\n          </el-form-item>\r\n           <el-form-item label=\"备注\"  prop=\"comment\">\r\n            <el-input v-model=\"editClientInfo.comment\"></el-input>\r\n          </el-form-item>\r\n        </el-form>\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"editDialogVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"editFormSubmit\">确 定</el-button>\r\n        </span>\r\n      </el-dialog>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    data(){\r\n      return{\r\n        tokenStr:'',\r\n        queryInfo:{\r\n          token:'',\r\n          query:'',\r\n          page:1,\r\n          pageSize:5\r\n        },\r\n        total:0,\r\n        tableData:[],\r\n        addDialogVisible:false,\r\n        editDialogVisible:false,\r\n       addClientInfo:{\r\n         storage_id:'',\r\n         supply_name:'',\r\n         cpmc:'',\r\n         in_number:'',\r\n         add_date:'',\r\n         comment:''\r\n        },\r\n        editClientInfo:{\r\n        id:'',\r\n         storage_id:'',\r\n         supply_name:'',\r\n         cpmc:'',\r\n         in_number:'',\r\n         add_date:'',\r\n         comment:''\r\n        },\r\n        selectStorageOption:[],\r\n       \r\n      }\r\n    },\r\n    created(){\r\n        this.tokenStr = window.sessionStorage.getItem('token');\r\n        this.getDataList();\r\n        this.selectStorage();\r\n    },\r\n    methods:{\r\n      async getDataList(){\r\n        this.queryInfo.token =this.tokenStr\r\n      const{data:res} = await this.$http.post('InstorageList',this.queryInfo)\r\n      console.log(res.data.result)\r\n      this.tableData = res.data.result\r\n      this.total = res.data.total\r\n      },\r\n      // 点击翻页重新发起数据请求\r\n    handleCurrentChange(newPage){\r\n        this.queryInfo.page = newPage;\r\n        this.getDataList();\r\n    },\r\n   async addFormSubmit(){\r\n        await this.$http.post('AddInStorageInfo',this.addClientInfo)\r\n        this.$refs.addClientInfo.resetFields();\r\n        this.getDataList();\r\n        this.addDialogVisible =false;\r\n        // console.log(this.$refs)\r\n\r\n    },\r\n   async editInfo(id){\r\n        // console.log(id)\r\n        this.editClientInfo.id = id;\r\n     const{data:res} = await this.$http.post('EditInStorageInfo',{id:id})\r\n     console.log(res.data)\r\n    //  this.editClientInfo.storage_id = res.data.storage_id;\r\n    //  this.editClientInfo.supply_name = res.data.supply_name;\r\n    //  this.editClientInfo.cpmc = res.data.cpmc;\r\n    //  this.editClientInfo.in_number = res.data.in_number;\r\n    //  this.editClientInfo.add_date = res.data.add_date;\r\n    //  this.editClientInfo.comment= res.data.comment;\r\n    this.editClientInfo.storage_id = res.data.storage_id;\r\n    this.editClientInfo.supply_name = res.data.supply_name;\r\n    this.editClientInfo.cpmc = res.data.cpmc;\r\n    this.editClientInfo.in_number = res.data.in_number;\r\n    this.editClientInfo.add_date = res.data.add_date;\r\n    this.editClientInfo.comment = res.data.comment;\r\n     this.editDialogVisible =true;\r\n    },\r\n    // 新增弹窗，获取仓库的下拉框数据\r\n    async selectStorage(){\r\n      const{data:res} = await this.$http.post('SelectZdStorageInfo');\r\n      console.log(res)\r\n      this.selectStorageOption = res.data;\r\n    },\r\n   async editFormSubmit(){\r\n     await this.$http.post('DoEditInStorageInfo',this.editClientInfo)\r\n      this.editDialogVisible =false;\r\n      this.$message.success('编辑成功')\r\n      //  this.$refs['addFormRef'].resetFields();\r\n      this.getDataList();\r\n    },\r\n    delInfo(id){\r\n            this.$confirm('此操作将永久删除该信息, 是否继续?', '提示', {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning'\r\n        }).then(() => {\r\n          this.$http.post('DelInStorageInfo',{\r\n            id:id\r\n          })\r\n          this.$message({\r\n            type: 'success',\r\n            message: '删除成功!'\r\n          });\r\n          this.getDataList();\r\n        }).catch(() => {\r\n          this.$message({\r\n            type: 'info',\r\n            message: '已取消删除'\r\n          });          \r\n        });\r\n      \r\n    }\r\n\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n.el-breadcrumb {\r\n  height: 30px;\r\n}\r\n.el-pagination {\r\n  margin-top: 10px;\r\n}\r\n.el-form .label{\r\n  font-size:15px!important;\r\n}\r\n</style>\r\n\r\n"],"sourceRoot":"src/components/enterprise/storageManage"}]}