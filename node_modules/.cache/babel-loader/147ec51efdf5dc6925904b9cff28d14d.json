,CAAA,YAAA,EAAA,KAAA,CAAA,SAAA,CAFA;;AAAA;AAAA;AAEA,gBAAA,GAFA,yBAEA,IAFA;AAGA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,IAAA,CAAA,MAAA;AACA,gBAAA,KAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA,CAAA,MAAA;AACA,gBAAA,KAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAMA,KAPA;AAQA;AACA,IAAA,mBATA,+BASA,OATA,EASA;AACA,WAAA,SAAA,CAAA,IAAA,GAAA,OAAA;AACA,WAAA,WAAA;AACA,KAZA;AAaA,IAAA,aAbA,2BAaA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,eAAA,EAAA,MAAA,CAAA,aAAA,CADA;;AAAA;AAEA,gBAAA,MAAA,CAAA,WAAA;;AACA,gBAAA,MAAA,CAAA,gBAAA,GAAA,KAAA;;AACA,gBAAA,MAAA,CAAA,KAAA,CAAA,UAAA,CAAA,WAAA;;AAJA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA,KAlBA;AAmBA,IAAA,QAnBA,oBAmBA,EAnBA,EAmBA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,gBAAA,EAAA;AAAA,kBAAA,EAAA,EAAA;AAAA,iBAAA,CAFA;;AAAA;AAAA;AAEA,gBAAA,GAFA,yBAEA,IAFA;AAGA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,IAAA;AACA,gBAAA,MAAA,CAAA,cAAA,GAAA,GAAA,CAAA,IAAA;AACA,gBAAA,MAAA,CAAA,iBAAA,GAAA,IAAA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAMA,KAzBA;AA0BA,IAAA,cA1BA,4BA0BA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,kBAAA,EAAA,MAAA,CAAA,cAAA,CADA;;AAAA;AAEA,gBAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;;AACA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,WAAA;;AAJA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA,KA/BA;AAgCA,IAAA,OAhCA,mBAgCA,EAhCA,EAgCA;AAAA;;AACA,WAAA,QAAA,CAAA,oBAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,eAAA,EAAA;AACA,UAAA,EAAA,EAAA;AADA,SAAA;;AAGA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,SADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;;AAIA,QAAA,MAAA,CAAA,WAAA;AACA,OAbA,EAaA,KAbA,CAaA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,MADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OAlBA;AAoBA;AArDA;AA9BA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n     <!-- 面包屑导航 -->\r\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n      <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>流通过程管理</el-breadcrumb-item>\r\n      <el-breadcrumb-item>客户信息表</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n    <el-card class=\"box-card\">\r\n    <!-- 搜索与添加区域 -->\r\n      <el-row :gutter=\"10\">\r\n        <el-col :span=\"6\">\r\n          <el-input\r\n            clearable\r\n            v-model=\"queryInfo.query\"\r\n            @clear=\"getDataList\"\r\n            placeholder=\"请输入投入品名称\"\r\n            class=\"input-with-select\"\r\n          >\r\n            <el-button\r\n              @click=\"getDataList\"\r\n              slot=\"append\"\r\n              icon=\"el-icon-search\"\r\n            ></el-button>\r\n          </el-input>\r\n        </el-col>\r\n        <el-col :span=\"2\">\r\n          <el-button type=\"primary\" @click=\"addDialogVisible = true\"\r\n            >新增客户信息</el-button\r\n          >\r\n        </el-col>\r\n      </el-row>\r\n      <!-- 数据列表 -->\r\n        <el-table\r\n      :data=\"tableData\"\r\n      style=\"width: 100%\">\r\n      <el-table-column\r\n        type=\"index\"\r\n        label=\"编号\"\r\n      >\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"client_firm\"\r\n        label=\"客户企业名称\"\r\n        width=\"180\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"client_name\"\r\n        label=\"客户姓名\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"client_phone\"\r\n        label=\"客户电话\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"client_source\"\r\n        label=\"客户来源\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"client_address\"\r\n        label=\"客户地址\">\r\n      </el-table-column>\r\n        <el-table-column label=\"操作\" width=\"180px\">\r\n          <!-- 插槽自定义样式 -->\r\n          <template slot-scope=\"scope\">\r\n            <el-tooltip effect=\"dark\" content=\"编辑\" placement=\"top-start\">\r\n              <el-button\r\n                @click=\"editInfo(scope.row.id)\"\r\n                type=\"primary\"\r\n                icon=\"el-icon-edit\"\r\n              ></el-button>\r\n            </el-tooltip>\r\n            <el-tooltip effect=\"dark\" content=\"删除\" placement=\"top-end\">\r\n              <el-button\r\n                type=\"warning\"\r\n                @click=\"delInfo(scope.row.id)\"\r\n                icon=\"el-icon-delete\"\r\n              ></el-button>\r\n            </el-tooltip>\r\n          </template>\r\n        </el-table-column>\r\n    </el-table>\r\n       <!-- 分页区域 -->\r\n      <el-pagination\r\n        background\r\n        layout=\"prev, pager, next,total\"\r\n        :total=\"total\"\r\n        :page-size=\"queryInfo.pageSize\"\r\n        @current-change=\"handleCurrentChange\"\r\n      >\r\n      </el-pagination>\r\n       <!-- 新增投入品台账dialog -->\r\n      <el-dialog\r\n        title=\"新增客户信息\"\r\n        :visible.sync=\"addDialogVisible\"\r\n        width=\"40%\"\r\n      >\r\n        <!-- 表单区 -->\r\n        <el-form size=\"medium\"  ref=\"addFormRef\" :inline=\"true\" :model=\"addClientInfo\" label-width=\"120px\">\r\n          <el-form-item label=\"客户公司名称\"  prop=\"trpmc\">\r\n            <el-input v-model=\"addClientInfo.client_firm\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"客户名称\"  prop=\"nyscz\">\r\n            <el-input v-model=\"addClientInfo.client_name\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"客户电话\"  prop=\"nydjzh\">\r\n            <el-input v-model=\"addClientInfo.client_phone\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"客户地址\"  prop=\"nyscdz\">\r\n            <el-input v-model=\"addClientInfo.client_address\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"客户来源\"  prop=\"nyzj\">\r\n            <el-input v-model=\"addClientInfo.client_source\"></el-input>\r\n          </el-form-item>\r\n        </el-form>\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"addDialogVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"addFormSubmit\">确 定</el-button>\r\n        </span>\r\n      </el-dialog>\r\n       <!-- 编辑投入品台账dialog -->\r\n       <el-dialog\r\n        title=\"新增客户信息\"\r\n        :visible.sync=\"editDialogVisible\"\r\n        width=\"40%\"\r\n      >\r\n        <!-- 表单区 -->\r\n        <el-form size=\"medium\"  ref=\"editFormRef\" :inline=\"true\" :model=\"editClientInfo\" label-width=\"120px\">\r\n          <el-form-item label=\"客户公司名称\"  prop=\"trpmc\">\r\n            <el-input v-model=\"editClientInfo.client_firm\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"客户名称\"  prop=\"nyscz\">\r\n            <el-input v-model=\"editClientInfo.client_name\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"客户电话\"  prop=\"nydjzh\">\r\n            <el-input v-model=\"editClientInfo.client_phone\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"客户地址\"  prop=\"nyscdz\">\r\n            <el-input v-model=\"editClientInfo.client_address\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"客户来源\"  prop=\"nyzj\">\r\n            <el-input v-model=\"editClientInfo.client_source\"></el-input>\r\n          </el-form-item>\r\n        </el-form>\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"editDialogVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"editFormSubmit\">确 定</el-button>\r\n        </span>\r\n      </el-dialog>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    data(){\r\n      return{\r\n        tokenStr:'',\r\n        queryInfo:{\r\n          token:'',\r\n          query:'',\r\n          page:1,\r\n          pageSize:5\r\n        },\r\n        total:0,\r\n        tableData:[],\r\n        addDialogVisible:false,\r\n        editDialogVisible:false,\r\n       addClientInfo:{\r\n         client_firm:'',\r\n         client_name:'',\r\n        client_phone:'',\r\n        client_address:'',\r\n        client_source:''\r\n        },\r\n        editClientInfo:{\r\n\r\n        },\r\n      }\r\n    },\r\n    created(){\r\n        this.tokenStr = window.sessionStorage.getItem('token');\r\n        this.getDataList();\r\n    },\r\n    methods:{\r\n      async getDataList(){\r\n        this.queryInfo.token =this.tokenStr\r\n      const{data:res} = await this.$http.post('ClientList',this.queryInfo)\r\n      console.log(res.data.result)\r\n      this.tableData = res.data.result\r\n      this.total = res.data.total\r\n      },\r\n      // 点击翻页重新发起数据请求\r\n    handleCurrentChange(newPage){\r\n        this.queryInfo.page = newPage;\r\n        this.getDataList();\r\n    },\r\n   async addFormSubmit(){\r\n        await this.$http.post('AddClientInfo',this.addClientInfo)\r\n        this.getDataList();\r\n        this.addDialogVisible =false;\r\n        this.$refs.addFormRef.resetFields();\r\n    },\r\n   async editInfo(id){\r\n        // console.log(id)\r\n     const{data:res} = await this.$http.post('EditClientInfo',{id:id});\r\n     console.log(res.data)\r\n     this.editClientInfo = res.data;\r\n     this.editDialogVisible =true;\r\n    },\r\n   async editFormSubmit(){\r\n     await this.$http.post('DoEditClientInfo',this.editClientInfo)\r\n      this.editDialogVisible =false;\r\n      this.$message.success('编辑成功')\r\n      this.getDataList();\r\n    },\r\n    delInfo(id){\r\n            this.$confirm('此操作将永久删除该信息, 是否继续?', '提示', {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning'\r\n        }).then(() => {\r\n          this.$http.post('DelClientInfo',{\r\n            id:id\r\n          })\r\n          this.$message({\r\n            type: 'success',\r\n            message: '删除成功!'\r\n          });\r\n          this.getDataList();\r\n        }).catch(() => {\r\n          this.$message({\r\n            type: 'info',\r\n            message: '已取消删除'\r\n          });          \r\n        });\r\n      \r\n    }\r\n\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n.el-breadcrumb {\r\n  height: 30px;\r\n}\r\n.el-pagination {\r\n  margin-top: 10px;\r\n}\r\n.el-form .label{\r\n  font-size:15px!important;\r\n}\r\n</style>\r\n\r\n"],"sourceRoot":"src/components/enterprise/ltgc"}]}