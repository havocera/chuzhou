,cAAA,UAAA,EAAA;AAFA;AAJA,WAXA;AAoBA,UAAA,IAAA,EAAA;AACA,YAAA,MAAA,EAAA,CACA;AACA,cAAA,IAAA,EAAA,OADA;AAEA,cAAA,UAAA,EAAA,CAAA,IAAA,CAAA,EAAA,GAAA,CAFA;AAGA,cAAA,QAAA,EAAA,IAAA,CAAA,EAAA,GAAA,GAHA;AAIA,cAAA,YAAA,EAAA,EAJA;AAKA,cAAA,MAAA,EAAA,KALA;AAMA,cAAA,IAAA,EAAA,CACA;AAAA,gBAAA,IAAA,EAAA,MAAA;AAAA,gBAAA,KAAA,EAAA,YAAA,CAAA,EAAA,EAAA,EAAA;AAAA,eADA,CANA;AASA,cAAA,SAAA,EAAA;AACA,gBAAA,IAAA,EAAA;AADA,eATA;AAYA,cAAA,QAAA,EAAA;AACA,gBAAA,IAAA,EAAA;AADA,eAZA;AAeA,cAAA,OAAA,EAAA;AACA,gBAAA,IAAA,EAAA;AADA,eAfA;AAkBA,cAAA,aAAA,EAAA;AACA,gBAAA,KAAA,EAAA;AACA,kBAAA,MAAA,EAAA;AADA;AADA,eAlBA;AAuBA,cAAA,OAAA,EAAA;AACA,gBAAA,IAAA,EAAA,IADA;AAEA,gBAAA,SAAA,EAAA,gBAFA;AAGA,gBAAA,KAAA,EAAA;AACA,kBAAA,IAAA,EAAA,SADA;AAEA,kBAAA,QAAA,EAAA;AAFA;AAHA;AAvBA,aADA,CADA;AAmCA,YAAA,KAAA,EAAA,CAAA,SAAA;AAnCA;AApBA,SAAA;AAAA,OAAA,CAAA;AA0DA,KA9DA;AA+DA,IAAA,YA/DA,wBA+DA,MA/DA,EA+DA,MA/DA,EA+DA;AACA,UAAA,SAAA,CAAA,MAAA,KAAA,CAAA,EAAA;AACA,eAAA,QAAA,CAAA,IAAA,CAAA,MAAA,KAAA,MAAA,GAAA,CAAA,EAAA,EAAA,CAAA;AACA,OAFA,MAEA;AACA,eAAA,QAAA,CAAA,IAAA,CAAA,MAAA,MAAA,MAAA,GAAA,MAAA,GAAA,CAAA,IAAA,MAAA,EAAA,EAAA,CAAA;AACA;AACA;AArEA,GAPA;AA8EA,EAAA,OA9EA,qBA8EA;AACA,QAAA,UAAA,GAAA,IAAA,CAAA,UAAA;AAEA,IAAA,UAAA;AAEA,IAAA,WAAA,CAAA,KAAA,UAAA,EAAA,KAAA,CAAA;AACA;AApFA,CAAA","sourcesContent":["<template>\n  <div id=\"cards\">\n    <div\n      class=\"card-item\"\n      v-for=\"(card, i) in cards\"\n      :key=\"card.title\"\n    >\n      <div class=\"card-header\">\n        <div class=\"card-header-left\">{{ card.title }}</div>\n        <div class=\"card-header-right\">{{ '0' + (i + 1) }}</div>\n      </div>\n      <dv-charts class=\"ring-charts\" :option=\"card.ring\" />\n      <div class=\"card-footer\">\n        <div class=\"card-footer-item\">\n          <div class=\"footer-title\">累计金额</div>\n          <div class=\"footer-detail\">\n            <dv-digital-flop :config=\"card.total\" style=\"width:70%;height:35px;\" />元\n          </div>\n        </div>\n        <div class=\"card-footer-item\">\n          <div class=\"footer-title\">销售地</div>\n          <div class=\"footer-detail\">\n            <dv-digital-flop :config=\"card.num\" style=\"width:70%;height:35px;\" />处\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'Cards',\n  data () {\n    return {\n      cards: []\n    }\n  },\n  methods: {\n    createData () {\n      const { randomExtend } = this\n\n      this.cards = new Array(5).fill(0).map((foo, i) => ({\n        title:  (i + 6) +'月份销售信息',\n        total: {\n          number: [randomExtend(90000, 100000)],\n          content: '{nt}',\n          textAlign: 'right',\n          style: {\n            fill: '#ea6027',\n            fontWeight: 'bold'\n          }\n        },\n        num: {\n          number: [randomExtend(30, 60)],\n          content: '{nt}',\n          textAlign: 'right',\n          style: {\n            fill: '#26fcd8',\n            fontWeight: 'bold'\n          }\n        },\n        ring: {\n          series: [\n            {\n              type: 'gauge',\n              startAngle: -Math.PI / 2,\n              endAngle: Math.PI * 1.5,\n              arcLineWidth: 13,\n              radius: '80%',\n              data: [\n                { name: '资金占比', value: randomExtend(40, 60) }\n              ],\n              axisLabel: {\n                show: false\n              },\n              axisTick: {\n                show: false\n              },\n              pointer: {\n                show: false\n              },\n              backgroundArc: {\n                style: {\n                  stroke: '#224590'\n                }\n              },\n              details: {\n                show: true,\n                formatter: '上链产品占比{value}%',\n                style: {\n                  fill: '#1ed3e5',\n                  fontSize: 20\n                }\n              }\n            }\n          ],\n          color: ['#03d3ec']\n        }\n      }))\n    },\n    randomExtend (minNum, maxNum) {\n      if (arguments.length === 1) {\n        return parseInt(Math.random() * minNum + 1, 10)\n      } else {\n        return parseInt(Math.random() * (maxNum - minNum + 1) + minNum, 10)\n      }\n    }\n  },\n  mounted () {\n    const { createData } = this\n\n    createData()\n\n    setInterval(this.createData, 30000)\n  }\n}\n</script>\n\n<style lang=\"less\">\n#cards {\n  display: flex;\n  justify-content: space-between;\n  height: 45%;\n\n  .card-item {\n    background-color: rgba(6, 30, 93, 0.5);\n    border-top: 2px solid rgba(1, 153, 209, .5);\n    width: 19%;\n    display: flex;\n    flex-direction: column;\n  }\n\n  .card-header {\n    display: flex;\n    height: 20%;\n    align-items: center;\n    justify-content: space-between;\n\n    .card-header-left {\n      font-size: 18px;\n      font-weight: bold;\n      padding-left: 20px;\n    }\n\n    .card-header-right {\n      padding-right: 20px;\n      font-size: 40px;\n      color: #03d3ec;\n    }\n  }\n\n  .ring-charts {\n    height: 55%;\n  }\n\n  .card-footer {\n    height: 25%;\n    display: flex;\n    align-items: center;\n    justify-content: space-around;\n  }\n\n  .card-footer-item {\n    padding: 5px 10px 0px 10px;\n    box-sizing: border-box;\n    width: 40%;\n    background-color: rgba(6, 30, 93, 0.7);\n    border-radius: 3px;\n\n    .footer-title {\n      font-size: 15px;\n      margin-bottom: 5px;\n    }\n\n    .footer-detail {\n      font-size: 20px;\n      color: #1294fb;\n      display: flex;\n      font-size: 18px;\n      align-items: center;\n\n      .dv-digital-flop {\n        margin-right: 5px;\n      }\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src/components/gov/datav/datav"}]}