A,KAAA,CAAA,SAAA,CAAA,KAAA,GAAA,KAAA,CAAA,QAAA;AADA;AAAA,uBAEA,KAAA,CAAA,KAAA,CAAA,IAAA,CACA,sBADA,EAEA,KAAA,CAAA,SAFA,CAFA;;AAAA;AAAA;AAEA,gBAAA,GAFA,yBAEA,IAFA;AAMA,gBAAA,KAAA,CAAA,YAAA,GAAA,GAAA,CAAA,IAAA,CAAA,MAAA;AACA,gBAAA,KAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,YAAA;;AATA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAUA,KAdA;AAeA,IAAA,aAfA,2BAeA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,WAAA,CAAA,KAAA,GAAA,MAAA,CAAA,QAAA;AADA;AAAA,uBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,yBAAA,EAAA,MAAA,CAAA,WAAA,CAFA;;AAAA;AAGA,gBAAA,MAAA,CAAA,gBAAA,GAAA,KAAA;;AACA,gBAAA,MAAA,CAAA,KAAA,CAAA,UAAA,CAAA,WAAA;;AACA,gBAAA,MAAA,CAAA,WAAA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAMA,KArBA;AAsBA,IAAA,QAtBA,oBAsBA,EAtBA,EAsBA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,0BAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,QADA;AACA,kBAAA,EAAA,EAAA;AADA,iBAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,yBACA,IADA;AAIA,gBAAA,MAAA,CAAA,YAAA,GAAA,GAAA,CAAA,IAAA;AACA,gBAAA,MAAA,CAAA,iBAAA,GAAA,IAAA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KA7BA;AA8BA,IAAA,cA9BA,4BA8BA;AACA,WAAA,YAAA,CAAA,KAAA,GAAA,KAAA,QAAA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,4BAAA,EACA,KAAA,YADA;AAGA,WAAA,iBAAA,GAAA,KAAA;AACA,WAAA,WAAA;AAEA,KAtCA;AAuCA;AACA,IAAA,mBAxCA,+BAwCA,OAxCA,EAwCA;AACA;AACA,WAAA,SAAA,CAAA,IAAA,GAAA,OAAA;AACA,WAAA,WAAA;AACA,KA5CA;AA6CA,IAAA,OA7CA,mBA6CA,EA7CA,EA6CA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,QAAA,CAAA,sBAAA,EAAA,IAAA,EAAA;AACA,kBAAA,iBAAA,EAAA,IADA;AAEA,kBAAA,gBAAA,EAAA,IAFA;AAGA,kBAAA,IAAA,EAAA;AAHA,iBAAA,EAIA,KAJA,CAIA,UAAA,GAAA;AAAA,yBAAA,GAAA;AAAA,iBAJA,CADA;;AAAA;AACA,gBAAA,UADA;;AAAA,sBAMA,UAAA,IAAA,SANA;AAAA;AAAA;AAAA;;AAAA,kDAOA,MAAA,CAAA,QAAA,CAAA,IAAA,CAAA,MAAA,CAPA;;AAAA;AAAA;AAAA,uBASA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,yBAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,QADA;AACA,kBAAA,EAAA,EAAA;AADA,iBAAA,CATA;;AAAA;AASA,gBAAA,GATA;;AAAA,sBAYA,GAAA,CAAA,MAAA,KAAA,GAZA;AAAA;AAAA;AAAA;;AAAA,kDAaA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,YAAA,CAbA;;AAAA;AAeA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,YAAA;;AACA,gBAAA,MAAA,CAAA,WAAA;;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA,EAjBA,CAkBA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AAEA;AACA;AACA;AACA;AACA;AACA;;AAjCA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAkCA;AA/EA;AAhCA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 面包屑导航 -->\r\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n      <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>收购信息</el-breadcrumb-item>\r\n      <el-breadcrumb-item>上传原料收购信息</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n    <!-- 卡片视图区 -->\r\n    <el-card class=\"box-card\">\r\n      <!-- 搜索与添加区域 -->\r\n      <el-row :gutter=\"10\">\r\n        <el-col :span=\"6\">\r\n          <el-input\r\n            clearable\r\n            v-model=\"queryInfo.query\"\r\n            @clear=\"getDataList\"\r\n            placeholder=\"请输入原料名称\"\r\n            class=\"input-with-select\"\r\n          >\r\n            <el-button\r\n              @click=\"getDataList\"\r\n              slot=\"append\"\r\n              icon=\"el-icon-search\"\r\n            ></el-button>\r\n          </el-input>\r\n        </el-col>\r\n        <el-col :span=\"2\">\r\n          <el-button type=\"primary\" @click=\"addDialogVisible = true\"\r\n            >新增收购信息</el-button\r\n          >\r\n        </el-col>\r\n      </el-row>\r\n      <!-- 产品列表区域 -->\r\n      <el-table :data=\"dataInfoList\" style=\"width: 100%\">\r\n        <el-table-column label=\"编号\" type=\"index\"></el-table-column>\r\n        <el-table-column label=\"生产企业名称\" prop=\"qymc\"></el-table-column>\r\n        <el-table-column label=\"原料产地\" prop=\"chandi\"></el-table-column>\r\n        <el-table-column label=\"原料名称\" prop=\"ylmc\"></el-table-column>\r\n        <el-table-column label=\"经手人\" prop=\"jsr\"></el-table-column>\r\n        <el-table-column label=\"收购日期\" prop=\"sgrq\"></el-table-column>\r\n        <el-table-column label=\"操作\">\r\n          <!-- 插槽自定义样式 -->\r\n          <template slot-scope=\"scope\">\r\n            <el-tooltip effect=\"dark\" content=\"编辑\" placement=\"top-start\">\r\n              <el-button\r\n                @click=\"editInfo(scope.row.bh)\"\r\n                type=\"primary\"\r\n                icon=\"el-icon-edit\"\r\n              ></el-button>\r\n            </el-tooltip>\r\n            <el-tooltip effect=\"dark\" content=\"删除\" placement=\"top-end\">\r\n              <el-button\r\n                type=\"warning\"\r\n                @click=\"delInfo(scope.row.bh)\"\r\n                icon=\"el-icon-delete\"\r\n              ></el-button>\r\n            </el-tooltip>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <!-- 分页区域 -->\r\n      <el-pagination\r\n        background\r\n        layout=\"prev, pager, next,total\"\r\n        :total=\"total\"\r\n        :page-size=\"queryInfo.pageSize\"\r\n        @current-change =\"handleCurrentChange\"\r\n      >\r\n      </el-pagination>\r\n      <!-- 新增原料收购dialog -->\r\n      <el-dialog\r\n        title=\"新增收购信息\"\r\n        :visible.sync=\"addDialogVisible\"\r\n        width=\"30%\"\r\n      >\r\n        <!-- 表单区 -->\r\n        <el-form ref=\"addFormRef\" :model=\"addFormInfo\" label-width=\"80px\">\r\n          <el-form-item label=\"原料名称\" prop=\"ylmc\">\r\n            <el-input v-model=\"addFormInfo.ylmc\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"原料产地\" prop=\"chandi\">\r\n            <el-input v-model=\"addFormInfo.chandi\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"经手人\" prop=\"jsr\">\r\n            <el-input v-model=\"addFormInfo.jsr\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"备注信息\" prop=\"bz\">\r\n            <el-input v-model=\"addFormInfo.bz\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"收购日期\" prop=\"sgrq\">\r\n            <el-date-picker\r\n              value-format=\"yyyy-MM-dd\"\r\n              format=\"yyyy-MM-dd\"\r\n              v-model=\"addFormInfo.sgrq\"\r\n              type=\"date\"\r\n              placeholder=\"选择日期\"\r\n            >\r\n            </el-date-picker>\r\n          </el-form-item>\r\n        </el-form>\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"addDialogVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"addFormSubmit\">确 定</el-button>\r\n        </span>\r\n      </el-dialog>\r\n      <!-- 编辑原料收购dialog -->\r\n      <el-dialog\r\n        title=\"编辑收购信息\"\r\n        :visible.sync=\"editDialogVisible\"\r\n        width=\"30%\"\r\n      >\r\n        <!-- 表单区 -->\r\n        <el-form ref=\"editFormRef\" :model=\"editFormInfo\" label-width=\"80px\">\r\n          <el-form-item label=\"原料名称\">\r\n            <el-input v-model=\"editFormInfo.ylmc\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"原料产地\">\r\n            <el-input v-model=\"editFormInfo.chandi\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"经手人\">\r\n            <el-input v-model=\"editFormInfo.jsr\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"备注信息\">\r\n            <el-input v-model=\"editFormInfo.bz\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"收购日期\">\r\n            <el-date-picker\r\n              value-format=\"yyyy-MM-dd\"\r\n              format=\"yyyy-MM-dd\"\r\n              v-model=\"editFormInfo.sgrq\"\r\n              type=\"date\"\r\n              placeholder=\"选择日期\"\r\n            >\r\n            </el-date-picker>\r\n          </el-form-item>\r\n        </el-form>\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"editDialogVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"editFormSubmit\">确 定</el-button>\r\n        </span>\r\n      </el-dialog>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      tokenStr: \"\",\r\n      queryInfo: {\r\n        token: \"\",\r\n        query: \"\",\r\n        page: 1,\r\n        pageSize: 5\r\n      },\r\n      total: 0,\r\n      // 列表渲染数据\r\n      dataInfoList: [],\r\n      addDialogVisible: false,\r\n      addFormInfo: {\r\n        token:'',\r\n        ylmc: \"\",\r\n        chandi: \"\",\r\n        jsr: \"\",\r\n        sgrq:'',\r\n        bz: \"\"\r\n      },\r\n      editFormInfo:{\r\n\r\n      },\r\n      editDialogVisible:false,\r\n    };\r\n  },\r\n  created() {\r\n    this.getTokenStr();\r\n    this.getDataList();\r\n  },\r\n  methods: {\r\n    getTokenStr() {\r\n      this.tokenStr = window.sessionStorage.getItem(\"token\");\r\n    },\r\n    async getDataList() {\r\n      this.queryInfo.token = this.tokenStr;\r\n      const { data: res } = await this.$http.post(\r\n        \"MaterialpurchaseInfo\",\r\n        this.queryInfo\r\n      );\r\n      this.dataInfoList = res.data.result;\r\n      this.total = res.data.total;\r\n      console.log(res);\r\n      console.log(this.dataInfoList);\r\n    },\r\n   async addFormSubmit() {\r\n      this.addFormInfo.token = this.tokenStr;\r\n      await this.$http.post('AddMaterialpurchaseInfo',this.addFormInfo)\r\n      this.addDialogVisible = false;\r\n      this.$refs.addFormRef.resetFields();\r\n      this.getDataList();\r\n    },\r\n    async editInfo(id){\r\n        const {data:res} = await this.$http.post('EditMaterialpurchaseInfo',{\r\n          token:this.tokenStr,bh:id\r\n        })\r\n        this.editFormInfo = res.data;\r\n        this.editDialogVisible = true\r\n        console.log(res)\r\n    },\r\n    editFormSubmit(){\r\n        this.editFormInfo.token = this.tokenStr;\r\n      this.$http.post('DoEditMaterialpurchaseInfo',\r\n          this.editFormInfo\r\n        );\r\n        this.editDialogVisible = false;\r\n        this.getDataList();\r\n\r\n    },\r\n    // 分页功能--监听当前页码值 改变事件\r\n    handleCurrentChange(newPage) {\r\n      // console.log(\"当前页是\" + newPage);\r\n      this.queryInfo.page = newPage\r\n      this.getDataList();\r\n    },\r\n    async delInfo(id){\r\n        const confirmRes =await this.$confirm('此操作将永久删除该产地信息, 是否继续?', '提示', {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning'\r\n        }).catch(err=>err)\r\n        if(confirmRes !='confirm'){\r\n          return this.$message.info('取消删除')\r\n        }\r\n        const res = await this.$http.post('DelMaterialpurchaseInfo',{\r\n            token:this.tokenStr,bh:id\r\n          })\r\n        if(res.status !== 200){\r\n          return this.$message.error('删除原料收购信息失败')\r\n        }\r\n        this.$message.success('删除原料收购信息成功')\r\n        this.getDataList();\r\n        console.log(res)\r\n        // .then(() => {\r\n        //   this.$http.post('DelMaterialpurchaseInfo',{\r\n        //     token:this.tokenStr,bh:id\r\n        //   })\r\n        //   this.getDataList();\r\n        //   this.$message({\r\n        //     type: 'success',\r\n        //     message: '删除成功!'\r\n        //   });\r\n          \r\n        // }).catch(() => {\r\n        //   this.$message({\r\n        //     type: 'info',\r\n        //     message: '已取消删除'\r\n        //   });          \r\n        // });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n.el-breadcrumb {\r\n  height: 30px;\r\n}\r\n.el-pagination {\r\n  margin-top: 10px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/enterprise/sgInfo"}]}