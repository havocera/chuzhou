r\n    <el-card class=\"box-card\">\r\n    <!-- 搜索与添加区域 -->\r\n      <el-row :gutter=\"10\">\r\n        <el-col :span=\"6\">\r\n          <el-input\r\n            clearable\r\n            v-model=\"queryInfo.query\"\r\n            @clear=\"getDataList\"\r\n            placeholder=\"请输入投入品名称\"\r\n            class=\"input-with-select\"\r\n          >\r\n            <el-button\r\n              @click=\"getDataList\"\r\n              slot=\"append\"\r\n              icon=\"el-icon-search\"\r\n            ></el-button>\r\n          </el-input>\r\n        </el-col>\r\n        <el-col :span=\"2\">\r\n          <el-button type=\"primary\" @click=\"addDialogVisible = true\"\r\n            >新增投入品</el-button\r\n          >\r\n        </el-col>\r\n      </el-row>\r\n      <!-- 数据列表 -->\r\n        <el-table\r\n      :data=\"tableData\"\r\n      style=\"width: 100%\">\r\n      <el-table-column\r\n        type=\"index\"\r\n        label=\"编号\"\r\n      >\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"qymc\"\r\n        label=\"生产企业名称\"\r\n        width=\"180\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"trpmc\"\r\n        label=\"投入品名称\">\r\n      </el-table-column>\r\n        <el-table-column\r\n        prop=\"nydjzh\"\r\n        label=\"投入品登记证号\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"nyscz\"\r\n        label=\"生产商\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"nyzw\"\r\n        label=\"作物范围\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        :show-overflow-tooltip=\"true\"\r\n        prop=\"nyfz\"\r\n        label=\"防治对象\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"nysyfa\"\r\n        label=\"使用方法\"\r\n         :show-overflow-tooltip=\"true\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"trpgg\"\r\n        label=\"规格\">\r\n      </el-table-column>\r\n            <el-table-column\r\n        pr